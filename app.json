{
  "expo": {
    "name": "app-qr-scanner",
    "slug": "app-qr-scanner",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/icon_logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "resizeMode": "contain",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.matias98cs.appqrscanner"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/icon01.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.matias98cs.appqrscanner"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/icons/icon02.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
          "recordAudioAndroid": true
        }
      ],
      "expo-sqlite",
      "expo-asset"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "066f1ec0-e194-4a5e-bf70-aaee9c4c4e85"
      }
    }
  }
}